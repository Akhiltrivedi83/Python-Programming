//USING WHILE LOOP:

x=1
y=1
print("Enter the number upto which term you have to find series: ")
n=int(input())
i=1
print(x)
print(y)
while i<=n:
    m=x+y
    print(m)
    x=y
    y=m
    i+=1






//USING FOR LOOP:

print("Enter the nth term of fibonacci series:")
n=int(input())
x=1
y=1
print(x)
print(y)
for i in range(1,n):
    m=x+y
    print(m)
    x=y
    y=m
